<p id="notice"><%= notice %></p>

<h1>Dispensaries</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>City</th>
      <th>Province</th>
      <th>Country</th>
      <th>Postal code</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Website</th>
      <th>Kind</th>
      <th>Description</th>
      <th>Lat</th>
      <th>Lng</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dispensaries.each do |dispensary| %>
      <tr>
        <td><%= dispensary.name %></td>
        <td><%= dispensary.city %></td>
        <td><%= dispensary.province %></td>
        <td><%= dispensary.country %></td>
        <td><%= dispensary.postal_code %></td>
        <td><%= dispensary.phone %></td>
        <td><%= dispensary.email %></td>
        <td><%= dispensary.website %></td>
        <td><%= dispensary.kind %></td>
        <td><%= dispensary.description %></td>
        <td><%= dispensary.lat %></td>
        <td><%= dispensary.lng %></td>
        <td><%= link_to 'Show', dispensary %></td>
        <td><%= link_to 'Edit', edit_dispensary_path(dispensary) %></td>
        <td><%= link_to 'Destroy', dispensary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dispensary', new_dispensary_path %>
